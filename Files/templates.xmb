|#*XMB TEMPLATE FILE*#|index_ticker|#*XMB TEMPLATE*#|
<script type="text/javascript">
//<![CDATA[
var delay = $tickerdelay;
var node = '';
var current = 0;
var contents = new Array();
var running = false;
var runid = '';
var tnode = '';

$contents

function nodechange(node, text) {
if(node.nodeType == 3) {
node = text;
}else{
node.innerHTML = text;
}
}

function tickertoggle() {
if(running === true) {
running = false;
window.clearInterval(runid);
nodechange(tnode, "$lang[startticker]");
} else {
tickerstart();
}
}

function tickerrun() {
nodechange(node, contents[current]);
if(current == contents.length-1) {
current = 0;
} else {
current++;
}
}

function tickerstart() {
node = document.getElementById("tickerdiv");
tnode = document.getElementById("tickertoggle");

nodechange(tnode, "$lang[stopticker]");

tickerrun();
runid = window.setInterval(tickerrun, delay, '');

running = true;
}

window.onload = tickerstart;
// ]]>
</script>
